{
  "name": "cypress-plugin-check",
  "description": "Cypress Plugin Check is a custom Cypress plugin that provides a convenient way to check if an element is present before performing actions like click, type, or other Cypress commands.",
  "version": "1.0.1-beta",
  "main": "src",
  "types": "src",
  "files": [
    "src",
    "commands.js"
  ],
  "release": {
    "branches": [
      "1.0",
      {
        "name": "master",
        "channel": "rc",
        "prerelease": "rc"
      }
    ]
  },
  "scripts": {
    "test": "cypress run",
    "start": "npx cypress run --record --key fae89ec8-d60a-4e55-b8d1-7199d0e318a1",
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "semantic-release": "semantic-release",
    "badges": "npx -p dependency-version-badge update-badge cypress"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lasithdilshan20/cypress-plugin-check.git"
  },
  "keywords": [
    "cypress",
    "cypress.io",
    "cypress-plugin",
    "cypress-check",
    "cypress-plugin-check",
    "cypress-softassert"
  ],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/lasithdilshan20/cypress-plugin-check/issues"
  },
  "homepage": "https://github.com/lasithdilshan20/cypress-plugin-check#readme",
  "dependencies": {
    "cypress": "12.17.2",
    "prettier": "^2.8.8",
    "semantic-release": "^21.0.5"
  }
}
